<?xml version="1.0" encoding="UTF-8"?>
<program>
    <element type="class" name="Element">
        <arg type="primarykey">name</arg>
          <attr_access type="attribute_access">+</attr_access>
          <attr_type type="attribute_type">String</attr_type>
        <arg type="function">Element</arg>
          <attr_access type="attribute_access">+</attr_access>
          <attr_type type="attribute_type">void</attr_type>
        <arg type="function">getName</arg>
          <attr_access type="attribute_access">+</attr_access>
          <attr_type type="attribute_type">String</attr_type>
        <arg type="function">rename</arg>
          <attr_access type="attribute_access">+</attr_access>
          <attr_type type="attribute_type">void</attr_type>
        <arg type="position_x">500</arg>
        <arg type="position_y">50</arg>
    </element>

    <element type="class" name="UMLAttribute">
        <arg type="primarykey">type</arg>
          <attr_access type="attribute_access">#</attr_access>
          <attr_type type="attribute_type">UMLClassifier</attr_type>
        <arg type="function">getType</arg>
          <attr_access type="attribute_access">+</attr_access>
          <attr_type type="attribute_type">UMLClassifier</attr_type>
        <arg type="position_x">50</arg>
        <arg type="position_y">300</arg>
    </element>


    <element type="class" name="UMLClassifier">
        <arg type="function">UMLClassifier</arg>
          <attr_access type="attribute_access">+</attr_access>
          <attr_type type="attribute_type">void</attr_type>
        <arg type="function">isUserDefined</arg>
          <attr_access type="attribute_access">+</attr_access>
          <attr_type type="attribute_type">boolean</attr_type>
        <arg type="position_x">500</arg>
        <arg type="position_y">300</arg>
    </element>

    <element type="class" name="ClassDiagram">
        <arg type="attribute">classes</arg>
          <attr_access type="attribute_access">+</attr_access>  
          <attr_type type="attribute_type">List</attr_type>
        <arg type="function">createClass</arg>
          <attr_access type="attribute_access">+</attr_access>
          <attr_type type="attribute_type">UMLClass</attr_type>
        <arg type="function">classifierForName</arg>
          <attr_access type="attribute_access">+</attr_access>
          <attr_type type="attribute_type">UMLClassifier</attr_type>
        <arg type="position_x">950</arg>
        <arg type="position_y">300</arg>
    </element>
    
    <element type="class" name="UMLClass">
        <arg type="attribute">attributes</arg>
          <attr_access type="attribute_access">+</attr_access>  
          <attr_type type="attribute_type">List</attr_type>
        <arg type="function">addAttribute</arg>
          <attr_access type="attribute_access">+</attr_access>
          <attr_type type="attribute_type">boolean</attr_type>
        <arg type="function">getAttrPosition</arg>
          <attr_access type="attribute_access">+</attr_access>
          <attr_type type="attribute_type">int</attr_type>
        <arg type="position_x">500</arg>
        <arg type="position_y">500</arg>
    </element>

    <element type="constraint" name=" " left = "1 .. N" right = "1" consType = "Association">
      <arg type="constraint_from">UMLAttribute</arg>
      <arg type="constraint_to">UMLClassifier</arg>
    </element>

    <element type="constraint" name=" " left = "1" right = "0 .. N" consType = "Association">
      <arg type="constraint_from">ClassDiagram</arg>
      <arg type="constraint_to">UMLClassifier</arg>
    </element>

    <element type="constraint" name=" " left = "1" right = "0 .. N" consType = "Association">
      <arg type="constraint_from">UMLClass</arg>
      <arg type="constraint_to">UMLAttribute</arg>
    </element>


    <element type="constraint" name=" " left = "" right = "" consType = "Generalization">
      <arg type="constraint_from">UMLClass</arg>
      <arg type="constraint_to">UMLClassifier</arg>
    </element>

    <element type="constraint" name=" " left = "" right = "" consType = "Generalization">
      <arg type="constraint_from">UMLClassifier</arg>
      <arg type="constraint_to">Element</arg>
    </element>

    <element type="constraint" name=" " left = "" right = "" consType = "Generalization">
      <arg type="constraint_from">UMLAttribute</arg>
      <arg type="constraint_to">Element</arg>
    </element>

    <element type="constraint" name=" " left = "" right = "" consType = "Generalization">
      <arg type="constraint_from">ClassDiagram</arg>
      <arg type="constraint_to">Element</arg>
    </element>

    <element type="operation" name="Element">
      <arg type="function_name">Element</arg>
      <arg type="parameter_name">name</arg>
      <arg type="parameter_type">String</arg>
    </element>

    <element type="operation" name="Element">
      <arg type="function_name">rename</arg>
      <arg type="parameter_name">name</arg>
      <arg type="parameter_type">String</arg>
    </element>

    <element type="operation" name="UMLClassifier">
      <arg type="function_name">UMLClassifier</arg>
      <arg type="parameter_name">name</arg>
      <arg type="parameter_type">String</arg>
    </element>

    <element type="operation" name="ClassDiagram">
      <arg type="function_name">createClass</arg>
      <arg type="parameter_name">name</arg>
      <arg type="parameter_type">String</arg>
    </element>

    <element type="operation" name="ClassDiagram">
      <arg type="function_name">classifierForName</arg>
      <arg type="parameter_name">name</arg>
      <arg type="parameter_type">String</arg>
    </element>

    <element type="operation" name="UMLClass">
      <arg type="function_name">addAttribute</arg>
      <arg type="parameter_name">attr</arg>
      <arg type="parameter_type">UMLAttribute</arg>
    </element>

    <element type="operation" name="UMLClass">
      <arg type="function_name">getAttrPosition</arg>
      <arg type="parameter_name">attr</arg>
      <arg type="parameter_type">UMLAttribute</arg>
    </element>

</program>
